<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaojianma.memoryexercise">

    <application
        android:allowBackup="true"
        android:icon="@drawable/yepeng"
        android:label="@string/app_name"
        android:roundIcon="@drawable/yepeng"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.NumberCodeActivity"
            android:label="数字编码"
            android:theme="@style/Widget.AppCompat.ActionBar"></activity>
        <activity
            android:name=".activity.LetterCodeActivity"
            android:label="字母编码"
            android:theme="@style/Widget.AppCompat.ActionBar"></activity>
    </application>

</manifest>